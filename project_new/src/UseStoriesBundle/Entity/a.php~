<?php

namespace UseStoriesBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * a
 *
 * @ORM\Table(name="a")
 * @ORM\Entity(repositoryClass="UseStoriesBundle\Repository\aRepository")
 */
class a
{
    /**
     * @var int
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }
}
